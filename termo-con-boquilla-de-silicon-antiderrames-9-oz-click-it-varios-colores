generate_rp_tag = false;
add_to_order_tag = false;
group_id = 0;

if (typeof rp_prod == 'undefined'){
rp_prod = {};
}
rp_prod.p1831022657 = {};

rp_prod.p1831022657.v5606780929 = {};

rp_prod.p1831022657.v5606780993 = {};

rp_prod.p1831022657.v5606781121 = {};

rp_prod.p1831022657.v5606781185 = {};

rp_prod.p1831022657.v5606781249 = {};

rp_prod.p1831022657.v5606864833 = {};



jQuery(document).ready(function(){
addRPHandler();
setTimeout(addRPHandler, 700);


function addRPHandler(){

// jQuery("[name='id'],.single-option-selector").unbind();
jQuery("[name='id'],.single-option-selector").change(function(){

var myForm = $('.product_rp_div.p1831022657').closest("form");
var idInput = myForm.find("[name='id']");
var myVariant = idInput.val();
var prodId = 1831022657;


group_id = rp_prod['p'+prodId]["v"+myVariant].rp_group_id;

//add recurring order widget 


if(group_id){
$('.product_rp_div.p1831022657, .newRecurringDiv, .recurring-box').show(); 
} else {
$('.product_rp_div.p1831022657, .newRecurringDiv, .recurring-box').hide();
}

if(!generate_rp_tag && group_id)
$('.product_rp_div.p1831022657').append('<script async class="generate_rp" src="https://ro.boldapps.net/recurring_settings/generate_rp?&shop_url=bebe2go.myshopify.com&group_id='+group_id+'&customer_id=&product_id='+prodId+'&variant_id='+myVariant+'"></'+''+'script>'); 

generate_rp_tag = true; 


//toggle buttons 


if(JSON.stringify(group_id) == null){
$('.product_rp_div.p1831022657').hide();
$('#customButton').hide();
$('.addtocart').show();
}
//add add_to_order button
if(!add_to_order_tag)
$('.product_rp_div.p1831022657').append('<script async class="add_to_order" src="https://ro.boldapps.net/recurring_settings/add_to_order?&shop_url=bebe2go.myshopify.com&group_id='+group_id+'&customer_id=&product_id='+prodId+'&variant_id='+myVariant+'"></'+''+'script>'); 

add_to_order_tag = true; 

//toggle add to order btn

if(JSON.stringify(group_id) == null){
$('.add_modal_btn').hide(); 
}else{
$('.add_modal_btn').show(); 
}
 
});
jQuery("[name='id']").change();
jQuery('.roSelector').trigger('change');
}
});
</script> 
