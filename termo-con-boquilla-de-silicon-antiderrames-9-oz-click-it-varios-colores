  <script>
    var selectCallback = function(variant, selector) {
      if (variant && variant.available) {
        // selected a valid variant
        $('#add-to-cart').removeClass('disabled').removeAttr('disabled'); // remove unavailable class from add-to-cart button, and re-enable button
        $('.options .price').html('<i class="fa fa-check green"></i> &nbsp;In Stock');
        if(variant.compare_at_price == null){
          $('.purchase .price').html('<strong>'+Shopify.formatMoney(variant.price, "$ {{amount}} MXN")+'</strong>');
        } else {
          $('.purchase .price').html('<strong>'+Shopify.formatMoney(variant.price, "$ {{amount}} MXN") + '</strong> <span class="compare_at_price">Precio de Lista <del>' + Shopify.formatMoney(variant.compare_at_price, "$ {{amount}} MXN") + '</del></span>');
        }
      } else {
        // variant doesn't exist
        $('#add-to-cart').addClass('disabled').attr('disabled', 'disabled'); // set add-to-cart button to unavailable class and disable button
        $('.options .price').html('<i class="fa fa-times red"></i> &nbsp;Agotado');
      }
    };

